{
  "name": "@vk/blocks-vue",
  "version": "0.1.0",
  "description": "Vue 3 composables and components for vkecomblocks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@vk/blocks-core": "workspace:*"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "jsdom": "^24.1.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vitest": "^2.1.3"
  },
  "keywords": [
    "ecommerce",
    "cart",
    "vue",
    "vue3",
    "composables"
  ],
  "license": "MIT"
}

