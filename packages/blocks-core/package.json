{
  "name": "@vk/blocks-core",
  "version": "0.1.0",
  "description": "Core logic for vkecomblocks - framework agnostic",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ecommerce",
    "cart",
    "shopping",
    "framework-agnostic"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.7.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}

