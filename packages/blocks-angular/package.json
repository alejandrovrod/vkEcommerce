{
  "name": "@vk/blocks-angular",
  "version": "0.1.0",
  "description": "Angular services and components for vkecomblocks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@angular/core": ">=18.0.0",
    "@angular/common": ">=18.0.0"
  },
  "dependencies": {
    "@vk/blocks-core": "workspace:*"
  },
  "devDependencies": {
    "@angular/core": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@types/node": "^22.7.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "zone.js": "^0.15.0"
  },
  "keywords": [
    "ecommerce",
    "cart",
    "angular",
    "services"
  ],
  "license": "MIT"
}

