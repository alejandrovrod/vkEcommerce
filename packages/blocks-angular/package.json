{
  "name": "@vk/blocks-angular",
  "version": "0.1.0",
  "description": "Angular services and components for vkecomblocks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "type": "module",
  "peerDependencies": {
    "@angular/common": ">=21.0.0",
    "@angular/core": ">=21.0.0",
    "@angular/forms": ">=21.0.0"
  },
  "dependencies": {
    "@vk/blocks-core": "workspace:*"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.1.0",
    "@analogjs/vitest-angular": "^1.2.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@types/node": "^22.7.5",
    "jsdom": "^24.1.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9",
    "zone.js": "^0.15.0"
  },
  "keywords": [
    "ecommerce",
    "cart",
    "angular",
    "services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alejandrovrod/vkEcommerce.git",
    "directory": "packages/blocks-angular"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
